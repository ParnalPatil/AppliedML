   "source": [
    "Palindrome Number\n",
    "https://leetcode.com/problems/palindrome-number/"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "id": "7f85511f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Not Palindrome\n"
     ]
    }
   ],
   "source": [
    "x = -121\n",
    "ctr = 0\n",
    "k = str(x)\n",
    "n = len(k)\n",
    "for i in range(len(k)):\n",
    "    if(k[i] == k[n-i-1]):\n",
    "       continue\n",
    "    else:\n",
    "       ctr = 1\n",
    "       break\n",
    "if ctr == 0:\n",
    "    print('Palindrome')\n",
    "else:\n",
    "    print('Not Palindrome')"
   ]
  },
